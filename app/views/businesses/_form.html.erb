<%= render('layouts/errors', :object => @business) %>

<%= form_for [@category, @business] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :website_address %>
  <%= f.text_field :website_address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <p><%= f.submit %></p>
<% end %>
